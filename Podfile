# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'CodableExample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  

  # Pods for CodableExample
  pod 'Alamofire'
  pod 'MoreCodable', :git => 'https://github.com/tattn/MoreCodable.git'

  target 'CodableExampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CodableExampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
